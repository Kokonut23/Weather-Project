let latitude=console.log(position.coords.latitude);
    let longitude=console.log(position.coords.longitude);
    let apiKey="9d6241dfed0d2530cebec36187e08a92";
    let units="metric";
    let apiUrl=`https://api.openweathermap.org/data/2.5/weather?lat=${latitude}&${longitude}&appid=${apiKey}&units=${units}`;
    
    axios.get(apiUrl).then(showTemperature);


    
function showTemperature(response) {
    let currentTemperature=Math.round(response.data.main.temp);
    alert(`It is currently ${currentTemperature}°C`);
    let temperature=document.querySelector("#temperature");
    temperature.innerHTML=`${currentTemperature}°C`;
}








function showPosition (position) {
    alert (`Your latitude is ${position.coords.latitude} and you longitude is ${position.coords.longitude}`);
}

function findLocation (position) {
        navigator.geolocation.getCurrentPosition(showPosition);
}

let button=document.querySelector("#location");
button.addEventListener("click",findLocation);





function changeCity (event) {
          event.preventDefault();
          let cityInput=document.querySelector("#city-input");
          let h1=document.querySelector("h1");
          h1.innerHTML=cityInput.value;
        }
        

        function displayCurrentTemperature (response) {
    let weather=Math.round(response.data.main.temp);
    let currentCity=response.data.name;
    let heading=document.querySelector("h1");
    let temperatureWeather=document.querySelector("#temperature");
    heading.innerHTML=currentCity;
    temperature.innerHTML=`${temperature}°C`;
}



